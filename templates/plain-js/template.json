{
  "name": "JavaScript",
  "category": "js",
  "default": true,
  "dependencies": [
  ],
  "devDependencies": [
    "rollup",
    "rollup-plugin-node-resolve",
    "rollup-plugin-commonjs",
    "rollup-plugin-terser",
    "parcel-bundler",
    "jest",
    "supertest",
    "eslint",
    "eslint-config-airbnb-base",
    "eslint-plugin-import",
    "nodemon",
    "npm-run-all"
  ],
  "main" : "./dist/index.js",
  "scripts" : {
    "dev-rebuild": "node ./scripts/build.js --watch",
    "dev-restart": "node ./node_modules/nodemon/bin/nodemon --watch ./dist/ ./dist/index.js",
    "dev": "node ./node_modules/npm-run-all/bin/npm-run-all -p -r dev-rebuild dev-restart",
    "build": "node ./scripts/build.js",
    "start": "node ./dist/index.js",
    "lint": "node ./node_modules/eslint/bin/eslint . --ext .js",
    "lint-fix": "node ./node_modules/eslint/bin/eslint . --ext .js --fix",
    "test": "node ./node_modules/jest/bin/jest"
  },
  "postInstall": [
    "npm run build"
  ]
}